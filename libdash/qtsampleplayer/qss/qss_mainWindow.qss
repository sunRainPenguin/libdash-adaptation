QPushButton
{
	background-color: rgb(0, 170, 127);
	background-image: url(:/QtSamplePlayer/pic/backGround.jpg);
    font-family: "Aharoni";
    font-size: 14px;
    text-align: center;
    font-weight:100
}
QPushButton:hover, QPushButton:pressed , QPushButton:checked
{
    background-color: rgb(85, 170, 255);
    font-size: 16px;
    font-weight:200
}
QPushButton:disabled
{
	color: rgb(240, 240, 240);
	background-color: rgb(240, 240, 240);
	background-image: url(:/QtSamplePlayer/pic/backGround.jpg);
    font-family: "Aharoni";
    font-size: 14px;
    text-align: center;
    font-weight:100
}
